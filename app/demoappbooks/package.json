{
    "name": "demoappbooks",
    "version": "0.0.1",
    "description": "A demo Books App",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build": "ui5 build preload --clean-dest --include-task=generateCachebusterInfo",
        "start": "ui5 serve",
        "test": "npm run test:node && npm run test:java",
        "test:java": "karma start --server=java --single-run",
        "test:node": "karma start --server=node --single-run"
      },
      "keywords": [
        "ui5",
        "openui5",
        "sapui5"
      ],
      "devDependencies": {
        "@ui5/cli": "^3.8.0",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-ui5": "^3.0.4",
        "puppeteer": "^21.6.1",
        "ui5-middleware-simpleproxy": "^3.2.10",
        "ui5-task-zipper": "^3.1.1"
      }
    }